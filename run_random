
#! /bin/bash

for queue in Info MMInfty
do
    for order in 1 2 3 4
    do
      	for graph in erdos_renyi hypercube
        do
            for assign in item_first service_first 
            do
              	sbatch ../script.sh random_replacement.py --graph_type $graph --query_nodes 20 --order_moment $order --queue_type $queue --assign_order $assign
            done
        done
    done
done

